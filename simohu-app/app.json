{
  "expo": {
    "name": "simohu-app",
    "slug": "simohu-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/my-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielsousaonebox.simohuapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gabrielsousaonebox.simohuapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c7e71340-fbe5-45d6-ab65-dd03606badad"
      }
    },
    "owner": "gabriel-sousa-onebox",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/c7e71340-fbe5-45d6-ab65-dd03606badad"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/my-icon.png",
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/facil-alagoas-vetorizada-positivo.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ]
  }
}
